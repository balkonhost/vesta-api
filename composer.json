{
    "name": "kitteam/vesta-api",
    "description": "VestaCP client for Laravel",
    "keywords": [
        "framework",
        "laravel",
        "hosting",
        "vesta",
        "panel"
    ],
    "type": "library",
    "require": {
        "guzzlehttp/guzzle": "^6.2",
        "laravel/framework": "^5.0"
    },
    "license": "GPL-3.0",
    "support": {
        "issues": "https://github.com/kitteam/vesta-api/issues",
        "source": "https://github.com/kitteam/vesta-api"
    },
    "authors": [
        {
            "name": "Alexandr Chernyaev",
            "email": "bliz48rus@gmail.com"
        },
        {
            "name": "Anton Popov",
            "email": "a.popov@kit.team"
        }
    ],
    "autoload": {
        "psr-4": {
            "VestaAPI\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "VestaAPI\\Providers\\VestaServiceProvider"
            ],
            "aliases": {
                "Vesta": "VestaAPI\\Facades\\Vesta"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
